<script>
    const productLists = [{productNam: "Cà Phê Đen",productPrice: 10000},
                        {productNam: "Cà Phê Sữa",productPrice: 10000},
                        {productNam: "Cà Phê Muối",productPrice: 10000},
                        {productNam: "Bạc Xỉu",productPrice: 10000}];


    const productListElement = document.querySelector('.productList');


    if(productListElement) {
           let html = productLists.map((product,index)=> {

      return `<tr class='productItem'>
                                      <th scope="row">${index+1}</th>
                                      <td id="productName">${product.productNam}</td>
                                      <td><input type="number" min='0' value='0' class="amount" id="amount"></td>
                                      <td class='totalAmount' id='totalAmount'><span class='price'>0</span></td>
                                    </tr>`
    })

    html=html.concat(`    <tr>
                                      <th scope="row"></th>
                                      <td></td>
                                      <td></td>
                                      <td class='bold-text'>Tổng: <span class="finalMoney">0</span></td>
                                    </tr>`)

    productListElement.innerHTML = html.join('')
    }
   


</script>


    
